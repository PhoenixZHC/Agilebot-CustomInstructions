{
  "name": "CM",
  "type": "easyService",
  "scriptLang": "python",
  "description": "修改用户坐标系和工具坐标系的插件",
  "version": "0.1",
  "functions": {
    "SetTF": {
      "description": "工具坐标系",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [1, 2, 3, 4, 5, 6]
        },
        "Value": {
          "type": "float",
          "description": "参数值（数值类型）",
          "valueType": "number"
        }
      }
    },
    "SetUF": {
      "description": "用户坐标系",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [1, 2, 3, 4, 5, 6]
        },
        "Value": {
          "type": "float",
          "description": "参数值（数值类型）",
          "valueType": "number"
        }
      }
    },
    "SetTF_R": {
      "description": "工具坐标系（从R寄存器读取值）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [1, 2, 3, 4, 5, 6]
        },
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetUF_R": {
      "description": "用户坐标系（从R寄存器读取值）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [1, 2, 3, 4, 5, 6]
        },
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetTF_PR": {
      "description": "工具坐标系（从PR寄存器读取完整位姿）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "PR_ID": {
          "type": "int",
          "description": "PR寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetUF_PR": {
      "description": "用户坐标系（从PR寄存器读取完整位姿）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "PR_ID": {
          "type": "int",
          "description": "PR寄存器编号",
          "valueType": "number"
        }
      }
    }
  }
}
