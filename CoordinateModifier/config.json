{
  "name": "CM",
  "type": "easyService",
  "scriptLang": "python",
  "description": "用户自定义指令",
  "version": "1.0.0",
  "functions": {
    "SetTF": {
      "description": "工具坐标系",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [
            1,
            2,
            3,
            4,
            5,
            6
          ]
        },
        "Value": {
          "type": "float",
          "description": "参数值（数值类型）",
          "valueType": "number"
        }
      }
    },
    "SetUF": {
      "description": "用户坐标系",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [
            1,
            2,
            3,
            4,
            5,
            6
          ]
        },
        "Value": {
          "type": "float",
          "description": "参数值（数值类型）",
          "valueType": "number"
        }
      }
    },
    "SetTF_R": {
      "description": "工具坐标系（从R寄存器读取值）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [
            1,
            2,
            3,
            4,
            5,
            6
          ]
        },
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetUF_R": {
      "description": "用户坐标系（从R寄存器读取值）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "Pos": {
          "type": "select",
          "description": "位置参数编号（1=X, 2=Y, 3=Z, 4=A, 5=B, 6=C）",
          "options": [
            1,
            2,
            3,
            4,
            5,
            6
          ]
        },
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetTF_PR": {
      "description": "工具坐标系（从PR寄存器读取完整位姿）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "PR_ID": {
          "type": "int",
          "description": "PR寄存器编号",
          "valueType": "number"
        }
      }
    },
    "SetUF_PR": {
      "description": "用户坐标系（从PR寄存器读取完整位姿）",
      "parameters": {
        "ID": {
          "type": "int",
          "description": "ID号（1-30，0是基础坐标系不可修改）",
          "min": 1,
          "max": 30,
          "valueType": "number"
        },
        "PR_ID": {
          "type": "int",
          "description": "PR寄存器编号",
          "valueType": "number"
        }
      }
    },
    "Incr": {
      "description": "R寄存器自增",
      "parameters": {
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        },
        "Step": {
          "type": "float",
          "description": "自增步长，默认为1.0",
          "valueType": "number"
        }
      }
    },
    "Decr": {
      "description": "R寄存器自减",
      "parameters": {
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号",
          "valueType": "number"
        },
        "Step": {
          "type": "float",
          "description": "自减步长，默认为1.0",
          "valueType": "number"
        }
      }
    },
    "Strp": {
      "description": "拆解字符串数据到PR寄存器",
      "parameters": {
        "SR_ID": {
          "type": "int",
          "description": "字符串寄存器编号，格式：第一个值是状态位（0=错误，1=正确），后面是分隔符分隔的数据。支持自动检测分隔符（逗号、分号、竖线、空格、制表符等）。示例：\"1,100.5,200.3,300.1,45.2,60.8,90.0\"（逗号）或\"1|100.5|200.3|300.1|45.2|60.8|90.0\"（竖线）",
          "valueType": "number"
        },
        "R_ID": {
          "type": "int",
          "description": "R寄存器编号，用于输出状态位（0=数据错误，1=数据正确）",
          "valueType": "number"
        },
        "PR_ID": {
          "type": "int",
          "description": "PR寄存器起始编号，每个PR寄存器存储6个分量（X,Y,Z,A,B,C），超过6个数据依次保存在PR[PR_ID+1], PR[PR_ID+2]等",
          "valueType": "number"
        }
      }
    }
  }
}
